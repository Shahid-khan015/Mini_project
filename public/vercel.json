{
    "version": 2,
    "builds": [
      {
        "src": "api/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "api/index.php"
      },
      {
        "src": "/full_video",
        "dest": "api/full_video.php"
      },
      {
        "src": "/(.*).(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)",
        "dest": "public/$1"
      }
    ]
  }
  